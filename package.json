{
  "name": "sprint4",
  "version": "1.0.0",
  "description": "ToDoList with mongodb and testing",
  "main": "./src/backend/server.start.ts",
  "scripts": {
    "start": "node dist/src/backend/server.start.js",
    "development": "cross-env NODE_ENV=development nodemon index.js",
    "test": "cross-env NODE_ENV=test PORT=1234 jest --verbose --silent",
    "test:watch": "npm run test -- --watchAll",
    "lint": "eslint --ext .ts ./src",
    "lint:fix": "eslint --fix --ext .ts ./src",
    "dev": "NODE_ENV=dev ts-node-dev --inspect --respawn --pretty --transpile-only src/backend/server.start.ts",
    "build": "rimraf dist && tsc"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/jest": "27.5.2",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/mongodb": "^4.0.7",
    "@types/mongoose": "^5.11.97",
    "@types/mysql": "2.15.21",
    "@types/supertest": "^2.0.14",
    "eslint": "8.36.0",
    "eslint-config-codely": "2.1.3",
    "mysql": "2.18.1",
    "nodemon": "3.0.1",
    "rimraf": "4.4.1",
    "supertest": "6.3.3",
    "ts-jest": "28.0.0",
    "tsc-watch": "5.0.3",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "bcrypt": "5.1.1",
    "cors": "2.8.5",
    "crossenv": "^0.0.2-security",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "express-promise-router": "4.1.1",
    "helmet": "6.0.1",
    "jsonwebtoken": "9.0.2",
    "mongodb": "5.9.0",
    "mongoose": "7.6.0",
    "mysql2": "3.2.0"
  }
}
