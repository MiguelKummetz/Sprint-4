{
  "name": "sprint4",
  "version": "1.0.0",
  "description": "ToDoList with mongodb and testing",
  "main": "./src/backend/server.start.ts",
  "scripts": {
    "start": "cross-env NODE_ENV=production tsc && node dist/src/backend/server.start.js",
    "dev": "cross-env NODE_ENV=dev tsc && node dist/src/backend/server.start.js",
    "test": "cross-env NODE_ENV=test PORT=9000 jest --silent --maxWorkers=1 ./tests",
    "test:watch": "npm run test -- --watchAll",
    "build": "rimraf dist && tsc"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/jest": "27.5.2",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/mysql": "2.15.21",
    "@types/supertest": "^2.0.14",
    "cross-env": "^7.0.3",
    "eslint": "8.36.0",
    "eslint-config-codely": "2.1.3",
    "install": "^0.13.0",
    "mysql": "2.18.1",
    "nodemon": "3.0.1",
    "npm": "^10.2.1",
    "rimraf": "4.4.1",
    "supertest": "6.3.3",
    "ts-jest": "28.0.0",
    "tsc-watch": "5.0.3",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "bcrypt": "5.1.1",
    "cors": "2.8.5",
    "crossenv": "^0.0.2-security",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "express-promise-router": "4.1.1",
    "helmet": "6.0.1",
    "jsonwebtoken": "9.0.2",
    "mongodb": "5.9.0",
    "mongoose": "7.6.0",
    "mysql2": "3.2.0"
  }
}
